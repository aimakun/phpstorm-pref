<application>
  <component name="RemoteServers">
    <remote-server name="Docker" type="docker">
      <configuration>
        <option name="certificatesPath" value="$USER_HOME$/.docker/machine/machines/default" />
        <option name="composeExecPath" value="/usr/local/bin/docker-compose" />
        <option name="machine" value="default" />
      </configuration>
    </remote-server>
    <remote-server name="Docker2" type="docker">
      <configuration>
        <option name="certificatesPath" value="$USER_HOME$/.docker/machine/machines/default" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="/Users" />
              <option name="virtualMachinePath" value="/Users" />
            </DockerPathMappingImpl>
            <DockerPathMappingImpl>
              <option name="localPath" value="$USER_HOME$/Code/fundzone" />
              <option name="virtualMachinePath" value="/opt/phpcs_temp.tmp" />
            </DockerPathMappingImpl>
          </list>
        </option>
      </configuration>
    </remote-server>
    <remote-server name="Docker3" type="docker">
      <configuration>
        <option name="apiUrl" value="https://192.168.99.100:2376" />
        <option name="certificatesPath" value="$USER_HOME$/.docker/machine/machines/default" />
        <option name="machine" value="default" />
      </configuration>
    </remote-server>
  </component>
</application>